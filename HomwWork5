package javaapplication3;

import javax.swing.JOptionPane;

/**
 *
 * @author CTI
 */
public class JavaApplication3 {

    /**
     * @param args the command line arguments
     */
    public static void main(String[] args) {
     
    {
        // define the range
        int a[]= new int [5],x, b[]=new int [5];
            int y=0;   
        for (int i=0; i<3; i++) {
          a[i]=1+(int)(Math.random()*100);
        JOptionPane.showMessageDialog(null,a[i]);
        
        }
        for (int i=0; i<3; i++){
            b[i]=Integer.parseInt(JOptionPane.showInputDialog("  Enter the number"));
            if (a[i]==b[i]) y++;
            else {JOptionPane.showMessageDialog(null, "Error !!");
            break;}
    }
        if (y==3) JOptionPane.showMessageDialog(null, "Ok");
        else JOptionPane.showMessageDialog(null, "Try again");
    
}}}

  
        
       
